<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="ControlHospitalizacionMedico" table="control_hospitalizacion_medico">

		<property name="idMedico" type="java.lang.String"
			column="id_medico" not-null="true" />

		<property name="idHospitalizacion" type="java.sql.Blob"
			column="id_hospitalizacion" not-null="true" />

	</class>
</hibernate-mapping>
