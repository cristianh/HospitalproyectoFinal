<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="Urgencia" table="urgencia">

		<id name="idUrgencia" type="java.lang.String" column="id_urgencia">
			<generator class="assigned" />
		</id>

		<property name="horaUrgencia" type="java.sql.Time"
			column="hora_urgencia" not-null="false" />

		<property name="fechaUrgencia" type="java.util.Date"
			column="fecha_urgencia" not-null="false" />

	</class>
</hibernate-mapping>
