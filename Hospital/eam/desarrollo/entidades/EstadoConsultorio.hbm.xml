<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="EstadoConsultorio" table="estado_consultorio">

		<id name="idEstadoConsultorio" type="java.lang.String" column="id_estado_consultorio">
			<generator class="assigned" />
		</id>

		<property name="descripcionEstadoConsultorio" type="java.lang.String"
			column="descripcion_estado_consultorio" not-null="false" />

	</class>
</hibernate-mapping>
