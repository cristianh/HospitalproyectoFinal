<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="Departamento" table="departamento">

		<id name="idDepartamento" type="java.lang.String" column="id_departamento">
			<generator class="assigned" />
		</id>

		<property name="nombreDepartamento" type="java.lang.String"
			column="nombre_departamento" not-null="false" />

	</class>
</hibernate-mapping>
