<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="Remision" table="remision">

		<id name="idRemision" type="java.lang.String" column="id_remision">
			<generator class="assigned" />
		</id>

		<property name="idPaciente" type="java.lang.String"
			column="id_paciente" not-null="true" />

		<property name="descripcionRemision" type="java.lang.String"
			column="descripcion_remision" not-null="false" />

	</class>
</hibernate-mapping>
