<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="EstadoHospitalizacion" table="estado_hospitalizacion">

		<id name="idEstadoHospitalizacion" type="java.lang.String" column="id_estado_hospitalizacion">
			<generator class="assigned" />
		</id>

		<property name="descripcionEstadoHospitalizacion" type="java.lang.String"
			column="descripcion_estado_hospitalizacion" not-null="false" />

	</class>
</hibernate-mapping>
