<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="EstadoCita" table="estado_cita">

		<id name="idEstadoCita" type="java.lang.String" column="id_estado_cita">
			<generator class="assigned" />
		</id>

		<property name="descripcionEstadoCita" type="java.lang.String"
			column="descripcion_estado_cita" not-null="false" />

	</class>
</hibernate-mapping>
