<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="Historia" table="historia">

		<id name="idHistoria" type="java.lang.String" column="id_historia">
			<generator class="assigned" />
		</id>

		<property name="idCita" type="java.lang.String"
			column="id_cita" not-null="true" />

		<property name="idHistoriaPaciente" type="java.lang.String"
			column="id_historia_paciente" not-null="true" />

	</class>
</hibernate-mapping>
