<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="Procedimiento" table="procedimiento">

		<id name="idProcedimiento" type="java.sql.Blob" column="id_procedimiento">
			<generator class="assigned" />
		</id>

		<property name="nombreProcedimiento" type="java.lang.String"
			column="nombre_procedimiento" not-null="false" />

		<property name="procedimiento" type="java.lang.String"
			column="procedimiento" not-null="false" />

	</class>
</hibernate-mapping>
