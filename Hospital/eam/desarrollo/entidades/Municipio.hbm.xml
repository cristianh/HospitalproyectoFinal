<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="Municipio" table="municipio">

		<id name="idMunicipio" type="java.lang.String" column="id_municipio">
			<generator class="assigned" />
		</id>

		<property name="nombreMunicipio" type="java.lang.String"
			column="nombre_municipio" not-null="false" />

		<property name="idDepartamento" type="java.lang.String"
			column="id_departamento" not-null="true" />

	</class>
</hibernate-mapping>
