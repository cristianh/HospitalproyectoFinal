<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="Consultorio" table="consultorio">

		<id name="idConsultario" type="java.lang.String" column="id_consultario">
			<generator class="assigned" />
		</id>

		<property name="descripcionConsultorio" type="java.lang.String"
			column="descripcion_consultorio" not-null="false" />

		<property name="idEstadoConsultorio" type="java.lang.String"
			column="id_estado_consultorio" not-null="true" />

	</class>
</hibernate-mapping>
