<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="eam.desarrollo.entidades">

	<class name="Especialidad" table="especialidad">

		<id name="idEspecialidad" type="java.lang.String" column="id_especialidad">
			<generator class="assigned" />
		</id>

		<property name="descripcionEspecialidad" type="java.lang.String"
			column="descripcion_especialidad" not-null="false" />

		<property name="idIncapacidad" type="java.lang.String"
			column="id_incapacidad" not-null="true" />

	</class>
</hibernate-mapping>
